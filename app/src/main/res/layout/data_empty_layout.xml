<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_edf0ee">

    <ImageView
        android:id="@+id/empty_top"
        android:layout_width="match_parent"
        android:layout_height="@dimen/net_error_layout_height"
        android:src="@drawable/no_wrong_data"/>

    <TextView
        android:id="@+id/empty_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/empty_top"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/net_error_layout_margintop"
        android:text="@string/no_wrong_data"
        android:textColor="@color/color_999999"
        android:textSize="@dimen/text_size_16" />

</RelativeLayout>
